<!-- launch RSM rviz plugins-->
<launch>
    <arg name="update_frequency" default="10" />

    <node pkg="rsm_rviz_plugins" type="waypointFollowingVisualizationNode" name="waypointFollowingVisualizationNode" output="screen">
        <param name="update_frequency" type="double" value="$(arg update_frequency)" />
    </node>
</launch>